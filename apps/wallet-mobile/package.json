{
  "name": "@yoroi/wallet-mobile",
  "version": "4.13.0",
  "private": true,
  "e2ePath": {
    "ios": "./e2e/tests/_ios",
    "android": "./e2e/tests/_android"
  },
  "setEnvDetox": "export TARGET_DEVICE_PLATFORM_NAME=iphonesimulator",
  "scripts": {
    "android-bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "build": "yarn lint && yarn tsc && yarn test",
    "e2e:build:android:dev:debug": "detox build --configuration android.emu.dev.debug",
    "e2e:test:android:dev:debug": "detox test $npm_package_e2ePath_android --configuration android.emu.dev.debug",
    "e2e:build:android:nightly:debug": "detox build --configuration android.emu.nightly.debug",
    "e2e:test:android:nightly:debug": "detox test $npm_package_e2ePath_android --configuration android.emu.nightly.debug",
    "e2e:build:android:nightly:release": "detox build --configuration android.emu.nightly.release",
    "e2e:test:android:nightly:release": "detox test $npm_package_e2ePath_android --configuration android.emu.nightly.release",
    "e2e:build:ios:yoroi:debug": "$npm_package_setEnvDetox && detox build --configuration ios.sim.yoroi.debug",
    "e2e:test:ios:yoroi:debug": "detox test $npm_package_e2ePath_ios --configuration ios.sim.yoroi.debug",
    "e2e:build:ios:nightly:debug": "$npm_package_setEnvDetox && detox build --configuration ios.sim.nightly.debug",
    "e2e:test:ios:nightly:debug": "detox test $npm_package_e2ePath_ios --configuration ios.sim.nightly.debug",
    "e2e:build:ios:nightly:release": "$npm_package_setEnvDetox && detox build --configuration ios.sim.nightly.release",
    "e2e:test:ios:nightly:release": "detox test $npm_package_e2ePath_ios --configuration ios.sim.nightly.release",
    "e2e:test:all:nightly:debug": "yarn e2e:test:android:nightly:debug & yarn e2e:test:ios:nightly:debug",
    "i18n:missed": "i18n-unused display-missed",
    "i18n:unused": "i18n-unused display-unused",
    "lint": "yarn lint:typescript && prettylint \"src/**/*.tsx\" \"src/**/*.ts\" --config .prettierrc",
    "lint:fix": "yarn lint:typescript --fix",
    "lint:typescript": "eslint --max-warnings=0 ./src -c ./.eslintrc.js",
    "manage-translations": "node ./translations/translation-runner.js",
    "metrics:pull": "ampli pull mobile",
    "metrics:status": "ampli status --instanceName=track",
    "prettify": "prettier --write ./src ./e2e ./storybook ./translations --config .prettierrc",
    "purge-translations": "rimraf ./translations/messages",
    "prerelease:android:nightly": "bundle install && ./scripts/update-commit.sh",
    "release:android:nightly": "bundle exec fastlane android release --env nightly",
    "prerelease:android:production": "bundle install && ./scripts/update-commit.sh",
    "release:android:production": "bundle exec fastlane android release --env production",
    "prerelease:ios:nightly": "bundle install && ./scripts/update-commit.sh && npx pod-install",
    "release:ios:nightly": "bundle exec fastlane ios release --env nightly",
    "prerelease:ios:production": "bundle install && ./scripts/update-commit.sh && npx pod-install",
    "release:ios:production": "bundle exec fastlane ios release --env production",
    "run:android:dev:debug": "ENVFILE=.env react-native run-android --mode=devDebug --appIdSuffix=dev --verbose",
    "run:android:nightly:debug": "ENVFILE=.env.nightly react-native run-android --mode=nightlyDebug --appIdSuffix=nightly --verbose",
    "run:android:nightly:release": "ENVFILE=.env.nightly react-native run-android --mode=nightlyRelease --appIdSuffix=nightly --verbose",
    "run:ios:dev:debug": "ENVFILE=.env react-native run-ios --mode=Debug --scheme=yoroi --verbose",
    "run:ios:nightly:debug": "ENVFILE=.env.nightly react-native run-ios --mode=Debug --scheme=nightly --verbose",
    "run:ios:nightly:release": "ENVFILE=.env.nightly react-native run-ios --mode=Release --scheme=nightly --verbose",
    "stag-ios": "react-native run-ios --scheme=emurgo-staging --configuration=Debug",
    "prestart": "sb-rn-get-stories",
    "start": "yarn react-native start --reset-cache --verbose",
    "storybook:watch": "sb-rn-watcher",
    "test": "jest src --ci --bail --silent",
    "tsc": "tsc --noEmit -p tsconfig.json",
    "tsc:cover": "yarn typescript-coverage-report -i '**/*.test.*' -i '**/*.stories*' -t html --threshold=100"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --cache --fix --max-warnings=0 -c ./.eslintrc.js",
      "prettier --write",
      "bash -c tsc -p tsconfig.json --noEmit"
    ],
    "*.{md,json}": "prettier --write",
    "*.stories.*": [
      "yarn sb-rn-get-stories",
      "git add ./.storybook"
    ]
  },
  "jest": {
    "cacheDirectory": ".jest/cache",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json",
        "babelConfig": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jestSetup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?(@react-native|react-native|react-native-iphone-x-helper|react-native-color-picker)/)"
    ]
  },
